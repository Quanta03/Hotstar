<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="@dimen/player_seek_bar_radius" />
			<solid android:color="@color/cw_tray_seekbar_bg" />
		</shape>
	</item>
	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="@dimen/player_seek_bar_radius" />
				<solid android:color="@color/cw_tray_seekbar_bg" />
			</shape>
		</clip>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="@dimen/player_seek_bar_radius" />
				<gradient android:startColor="@color/apple" android:endColor="@color/tree_green" />
			</shape>
		</clip>
	</item>
</layer-list>
